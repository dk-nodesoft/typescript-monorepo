{
  "name": "@dk-nodesoft/eslint-config",
  "private": true,
  "version": "1.5.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./helpers": {
      "require": "./src/helpers/index.js"
    },
    "./graphql-schema": {
      "require": "./src/bases/graphql-schema.js"
    },
    "./jest": {
      "require": "./src/bases/jest.js"
    },
    "./playwright": {
      "require": "./src/bases/playwright.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./regexp": {
      "require": "./src/bases/regexp.js"
    },
    "./tsdoc": {
      "require": "./src/bases/tsdoc.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/react.js"
    },
    "./tailwind": {
      "require": "./src/bases/tailwind.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.js"
    },
    "./turborepo": {
      "require": "./src/bases/turbo.js"
    },
    "./nestjs": {
      "require": "./src/frameworks/nestjs.js"
    },
    "./next": {
      "require": "./src/frameworks/next.js"
    },
    "./typescript-and-jest": {
      "require": "./src/frameworks/typescript-and-jest.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist ./.turbo ./.reports",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts",
    "lint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "fix-all-files": "eslint  --ext .ts,.tsx,.js,.jsx --fix"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@rushstack/eslint-patch": "1.5.1",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.3",
    "@types/prettier": "3.0.0",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-turbo": "1.10.15",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.16.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-storybook": "0.6.14",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-testing-library": "6.0.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "graphql": "16.8.1",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "typescript": "5.2.2"
  }
}
