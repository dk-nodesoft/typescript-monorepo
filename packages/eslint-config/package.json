{
  "name": "@dk-nodesoft/eslint-config",
  "private": true,
  "version": "1.5.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./helpers": {
      "require": "./src/helpers/index.js"
    },
    "./graphql-schema": {
      "require": "./src/bases/graphql-schema.js"
    },
    "./jest": {
      "require": "./src/bases/jest.js"
    },
    "./playwright": {
      "require": "./src/bases/playwright.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./regexp": {
      "require": "./src/bases/regexp.js"
    },
    "./tsdoc": {
      "require": "./src/bases/tsdoc.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/react.js"
    },
    "./tailwind": {
      "require": "./src/bases/tailwind.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.js"
    },
    "./turborepo": {
      "require": "./src/bases/turbo.js"
    },
    "./nestjs": {
      "require": "./src/frameworks/nestjs.js"
    },
    "./next": {
      "require": "./src/frameworks/next.js"
    },
    "./typescript-and-jest": {
      "require": "./src/frameworks/typescript-and-jest.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist ./.turbo ./.reports",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts",
    "lint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "fix-all-files": "eslint  --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "3.16.0",
    "@rushstack/eslint-patch": "1.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-turbo": "0.0.8",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.12.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-tailwindcss": "3.9.0",
    "eslint-plugin-testing-library": "5.10.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2"
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.14.2",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.35.0",
    "graphql": "16.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "eslint": "^8",
    "graphql": "^16",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^4"
  },
  "peerDependenciesMeta": {
    "qraphql": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
