    "@paralleldrive/cuid2": "2.1.0",
    "axios": "1.3.3",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@dk-nodesoft/eslint-config": "*",
    "@dk-nodesoft/jest-config": "*",
    "@dk-nodesoft/typescript-config": "*",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "nodemon": "2.0.20"
  }
